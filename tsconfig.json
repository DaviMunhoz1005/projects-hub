{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["react", "react-dom", "vite/client"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true
  },
  "include": ["src"]
}
